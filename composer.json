{
    "name": "Siphoc/Slic",
    "description": "Command Line Interface Micro Framework based on Symfony2 Components",
    "keywords": ["microframework","cli","symfony"],
    "homepage": "http://siphoc.github.com/slic",
    "license": "MIT",
    "authors":[
        {
            "name": "Jelmer Snoeck",
            "email": "jelmer@sihpoc.com"
        }
    ],
    "require":{
        "php": ">=5.3.2",
        "symfony/console": "2.1.*",
        "symfony/dependency-injection": "2.1.*",
        "monolog/monolog": ">=1.0.0",
        "symfony/finder": "2.1.*"
    },
    "autoload":{
        "psr-0":{
            "Slic": "src/"
        }
    },
    "extra":{
        "branch-alias":{
            "dev-master": "1.0-dev"
        }
    }
}
